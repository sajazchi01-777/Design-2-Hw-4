<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.design2hw4.SecondaryController">
   <children>
      <HBox layoutX="17.0" layoutY="187.0" prefHeight="25.0" prefWidth="515.0">
         <children>
            <TextField fx:id="txtName" promptText="Enter Name" />
            <ComboBox fx:id="cmbSubtype" prefWidth="150.0" promptText="Select Subtype">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </ComboBox>
            <DatePicker fx:id="dateRebirth" prefHeight="25.0" prefWidth="175.0" promptText="Select Date of Rebirth">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </DatePicker>
         </children>
      </HBox>
      <HBox layoutX="175.0" layoutY="14.0" prefHeight="25.0" prefWidth="200.0">
         <children>
            <Label prefHeight="53.0" prefWidth="209.0" text="Horror Character Creator">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox layoutX="251.0" layoutY="358.0" prefHeight="27.0" prefWidth="47.0" />
      <HBox layoutX="175.0" layoutY="285.0" prefHeight="25.0" prefWidth="200.0">
         <children>
            <ProgressBar fx:id="progBar" prefWidth="200.0" progress="0.0" />
         </children>
      </HBox>
      <Label fx:id="successText" layoutX="236.0" layoutY="310.0" prefHeight="18.0" prefWidth="77.0" />
      <Button fx:id="btnFinish" layoutX="20.0" layoutY="359.0" mnemonicParsing="false" text="Finish" onAction="#finishButtonPressed"/>
   </children>
</AnchorPane>
